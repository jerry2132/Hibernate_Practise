package com.hibernate.OneToMany3pm.controller;

import java.time.LocalDate;
import java.util.HashSet;
import java.util.Set;

import com.hibernate.OneToMany3pm.dao.StateDistrictDao;
import com.hibernate.OneToMany3pm.dto.District;
import com.hibernate.OneToMany3pm.dto.State;

public class InsertController {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		StateDistrictDao dao = new StateDistrictDao();
		
		Set<District> dist =new  HashSet<>();
		
		dist.add(new District(1,"Udham nagar",963742,LocalDate.parse("02-03-2004")));
		dist.add(new District(2,"jsdb",74152,LocalDate.parse("08-03-2124")));
		dist.add(new District(3,"hgjk",741,LocalDate.parse("12-063-19944")));
		dist.add(new District(4,"fyguh",62753,LocalDate.parse("06-08-2014")));
		dist.add(new District(5,"uyghjb",142152,LocalDate.parse("02-03-2004")));
		
		
		State st = new State(1,"UP","hab",24,dist);
		
		
		State state = dao.insertStateAndDistrict(st);
		
		System.out.println(state);
	}

}
